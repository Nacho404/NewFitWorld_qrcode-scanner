    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "electron": "^23.1.2",
    "electron-builder": "^24.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.9.4"
  }
}
