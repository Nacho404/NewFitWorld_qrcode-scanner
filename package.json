{
  "name": "fitness-center-qrcode-scanner",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "browser": "ng serve",
    "start": "npm-run-all -p electron:serve ng:serve:dev",
    "just-electron": "electron .",
    "electron:serve": "wait-on tcp:5200 && npx electron . --serve",
    "ng:serve:dev": "ng serve -c development --port 5200",
    "build": "ng build",
    "build:prod": "npm run build -- -c production",
    "watch": "ng build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.1",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@material/dialog": "^14.0.0",
    "@material/select": "^14.0.0",
    "angular-animations": "^0.11.0",
    "material-dialog": "^0.0.1",
    "material-icons": "^1.13.2",
    "ngx-scanner-qrcode": "^1.2.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "wait-on": "^7.0.1",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.3",
